{
    "env": {
        "BOLOS_SDK": "~/.ledger/sdk/nanos-secure-sdk",
        "BOLOS_ENV": "~/.ledger/env",
        "GCC_COMPILER_NAME": "gcc-arm-none-eabi-10.3-2021.10"
    },
    "configurations": [
        {
            "name": "Nano S",
            "includePath": [
                "${workspaceFolder}/src",
                "${env:BOLOS_ENV}/${env:GCC_COMPILER_NAME}/arm-none-eabi/include",
                "${env:BOLOS_SDK}/include/*",
                "${env:BOLOS_SDK}/lib_ux/include/*",
                "${env:BOLOS_SDK}/lib_cxng/include/*"
            ],
            "defines": [
                "TARGET_NANOS",
                "OS_IO_SEPROXYHAL",
                "HAVE_BAGL",
                "HAVE_ECC",
                "HAVE_ECC_WEIERSTRASS",
                "HAVE_SECP_CURVES",
                "HAVE_SPRINTF",
                "HAVE_HASH",
                "HAVE_BLAKE2",
                "HAVE_RNG",
                "HAVE_HMAC",
                "HAVE_SHA256",
                "HAVE_MATH",
                "HAVE_IO_USB",
                "HAVE_L4_USBLIB",
                "IO_USB_MAX_ENDPOINTS=6",
                "IO_HID_EP_LENGTH=64",
                "HAVE_USB_APDU",
                "USB_SEGMENT_SIZE=64",
                "UNUSED(x)=(void)x",
                "APPVERSION=\"1.0.0\"",
                "APPNAME=\"Ergo\"",
                "MAJOR_VERSION=1",
                "MINOR_VERSION=0",
                "PATCH_VERSION=0",
                "IO_SEPROXYHAL_BUFFER_SIZE_B=128",
                "HAVE_UX_FLOW",
                "DEBUG_BUILD=1",
                "HAVE_PRINTF",
                "PRINTF=screen_printf"
            ],
            "compilerPath": "${env:BOLOS_ENV}/${env:GCC_COMPILER_NAME}/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}